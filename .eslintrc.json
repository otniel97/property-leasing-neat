{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "require-jsdoc": "off",
        "max-len": "off",
        "linebreak-style": "off",
        "camelcase": "off",
        "no-extend-native": "off",
        "space-before-function-paren": "off",
        "object-curly-spacing": "off",
        "new-cap": "off",
        "padded-blocks": "off"
    }
}
